<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="20dp">

        <com.yc.roundcorner.view.RoundTextView
            android:id="@+id/tv_log_server1"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="left|center_vertical"
            android:paddingLeft="@dimen/dp20"
            android:text="1.log日志调试案例"
            android:textSize="@dimen/textSize18"
            android:background="@color/priority_green"
            app:rRadius="@dimen/dp20"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <com.yc.roundcorner.view.RoundTextView
            android:id="@+id/tv_log_server"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="left|center_vertical"
            android:paddingLeft="@dimen/dp20"
            android:text="2.log日志调试案例"
            android:textSize="@dimen/textSize18"
            android:background="@color/priority_green"
            android:layout_marginTop="@dimen/dp20"
            app:rRadius="@dimen/dp20"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_log_server1"/>


        <com.yc.roundcorner.view.RoundTextView
            android:id="@+id/tv_vibrator_test"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="left|center_vertical"
            android:paddingLeft="@dimen/dp20"
            android:text="3.手机震动的设置"
            android:textSize="@dimen/textSize18"
            android:background="@color/priority_green"
            android:layout_marginTop="@dimen/dp20"
            app:rRadius="@dimen/dp20"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_log_server"/>


        <com.yc.roundcorner.view.RoundTextView
            android:id="@+id/tv_private_test"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="left|center_vertical"
            android:paddingLeft="@dimen/dp20"
            android:text="4.隐私合规获取敏感信息"
            android:textSize="@dimen/textSize18"
            android:background="@color/priority_green"
            android:layout_marginTop="@dimen/dp20"
            app:rRadius="@dimen/dp20"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_vibrator_test"/>

        <com.yc.roundcorner.view.RoundTextView
            android:id="@+id/tv_net_test"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="left|center_vertical"
            android:paddingLeft="@dimen/dp20"
            android:text="5.网络请求案例大合集"
            android:textSize="@dimen/textSize18"
            android:background="@color/priority_green"
            android:layout_marginTop="@dimen/dp20"
            app:rRadius="@dimen/dp20"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_private_test"/>

        <com.yc.roundcorner.view.RoundTextView
            android:id="@+id/tv_thread_test"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="left|center_vertical"
            android:paddingLeft="@dimen/dp20"
            android:text="6.线程相关的案例"
            android:textSize="@dimen/textSize18"
            android:background="@color/priority_green"
            android:layout_marginTop="@dimen/dp20"
            app:rRadius="@dimen/dp20"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_net_test"/>


    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.core.widget.NestedScrollView>
