<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yc.appservice">

    <application
        android:name="com.yc.library.base.app.LibApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_background"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher_background"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ServiceActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name="com.yc.appservice.AppInfoService"
            android:enabled="true"
            android:process=":aidlservice"
            android:exported="true">
            <intent-filter>
                <action android:name="com.yc.appservice.AppInfoService.action" />
            </intent-filter>
        </service>
    </application>

</manifest>

